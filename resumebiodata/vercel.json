{
  "version": 2,
  "regions": ["bom1"],
  "functions": {
    "src/app/api/download-biodata/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/download-resume/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
